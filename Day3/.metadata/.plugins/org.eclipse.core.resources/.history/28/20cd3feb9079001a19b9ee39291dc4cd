public class BoxDyn {
	public void show(Object ob) {
		if (ob instanceof Integer) {
			System.out.println("Integer Casting...");
			int x=(Integer)ob;
		}
		if (ob instanceof Double) {
			
		}
	}
	public static void main(String[] args) {
		int a=12;
