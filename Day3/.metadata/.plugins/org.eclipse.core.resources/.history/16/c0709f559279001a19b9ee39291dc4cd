package com.zorin.day3;

public class Quiz1 {
	public static void main(String[] args) {
		  byte b = 5;
		    char c = 'a'; 
		    short s = -2; 
		    int i = 10; 
		    i = b; 
		    System.out.println(i);
		    i = c; 
		    System.out.println(i);
		    i = s;  
		    System.out.println(i);
	}
}
