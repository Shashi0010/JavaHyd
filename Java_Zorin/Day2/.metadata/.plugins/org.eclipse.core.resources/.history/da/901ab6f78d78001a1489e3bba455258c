package demo;

public class PosNeg {
	public void check(int n) {
		if(n==0) {
			System.out.println("Neutral Number");
		}
		else if(n > 0) {
			System.out.println("Positive Number...");
		} else {
			System.out.println("Negative Number...");
		}
	}
	public static void main(String[] args) {
		int n=0;
		PosNeg obj=new PosNeg();
		obj.check(n);
	}
}
